{
    "name": "victorbuild/sitemap",
    "description": "sitemap package for Laravel10 and PHP8",
    "keywords": ["laravel", "php", "sitemap", "generator", "xml", "html", "google-news"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rumen Damyanov",
            "email": "r@alfamatter.com",
            "role": "Developer",
            "homepage": "https://darumen.com"
        },
        {
            "name": "Victor",
            "email": "blog@vnewin.com",
            "role": "Developer",
            "homepage": "https://www.vnewin.com/"
        }
    ],
    "support": {
        "issues": "https://github.com/victorbuild/laravel-sitemap/issues",
        "source": "https://github.com/victorbuild/laravel-sitemap",
        "wiki": "https://github.com/victorbuild/laravel-sitemap/wiki"
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0",
        "illuminate/filesystem": "^10.0"
    },
    "require-dev": {
        "laravel/framework": "^10.0",
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench-core": "^6.0 || ^7.0",
        "php-coveralls/php-coveralls" : "^2.2"
    },
    "autoload": {
        "psr-0": {
            "Victorbuild\\Sitemap": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Victorbuild\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
